#:kivy 1.11.1
<Root>:
    orientation: 'vertical'
    id: root_box
    swiper_obj: swipe_carousel
    Swiper:
        id: swipe_carousel
        size_hint_y: 0.7
        direction: 'right'

    RelativeLayout:
        size_hint_y: 0.3
        Button:
            pos_hint: {'center_x': 0.5, 'center_y': 0.5}
            size_hint: (0.25,0.25)
